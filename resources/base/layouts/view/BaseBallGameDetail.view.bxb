result-view {
  //  Action에 Output Name  (binding Name)
  match: baseBallGameDetail (baseBallGameDetail) {
    from-output : getBaseBallDetail
    min (Required)
    max (Many)
  }
  message{
    template ("#{value(baseBallGameDetail.BaseBallSummary.AwayTeamName)} VS #{value(baseBallGameDetail.BaseBallSummary.HomeTeamName)} 경기 결과입니다.")
  }
  
